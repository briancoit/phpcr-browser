{
    "name": "marmelab/phpcr-browser",
    "description": "PHPCR Browser provides web interface to explore PHPCR repositories.",
    "keywords": ["phpcr", "Symfony CMF", "browser"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "silex/silex": "~1.1",
        "symfony/yaml": "^2.0|^3.0",
        "twig/twig": "^1.15",
        "symfony/twig-bridge": "^2.0|^3.0",
        "igorw/config-service-provider": "^1.2",
        "marmelab/phpcr-api-silex-provider": "^2.0",
        "jackalope/jackalope-jackrabbit": "^1.3"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "1.1.*"
    },
    "suggest": {
        "jackalope/jackalope-doctrine-dbal": "To connect to a Doctrine DBAL content repository, 1.1.*"
    },
    "autoload": {
        "psr-0": {"PHPCRBrowser\\": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
